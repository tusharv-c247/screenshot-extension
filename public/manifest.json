{
  "manifest_version": 3,
  "name": "Screenshot Extesnsion",
  "version": "1.0",
  "description": "Take screenshots of your current webpage",
  "permissions": ["activeTab", "scripting","tabs", "desktopCapture"],
  "host_permissions": [
  "<all_urls>"
],
"background": {
  "service_worker": "background.js"
},
  "action": {
    "default_popup": "index.html",
    "default_title": "Screenshot Extesnsion",
     "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png"
  }
}
